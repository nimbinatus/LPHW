def setcall(var1, var2):
    int(var1)
    int(var2)
    print "Setting the first variable to %d." % var1
    print "Setting the second variable to %d." % var2
    raw_input("What do you think the sum would be? ")
    varsum = var1 + var2
    print "The sum is %d." % varsum

# call 1: straight integers
setcall(1, 2)

# call 2: passed variables
x = 3
y = 5
setcall(x, y)

# call 3: raw input
a = int(raw_input("Set variable 1: "))
b = int(raw_input("Set variable 2: "))
setcall(a, b)

# call 4: combo int and var
c = 3
setcall (c, 4)

# call 5: combo int and raw
d = int(raw_input("Set variable 2: "))
setcall(7, d)

# call 6: combo var and raw
e = int(raw_input("Set variable 1: "))
f = 1
setcall(e, f)

# call 7: sums
setcall(1 + 2, 3 + 4)

# call 8: var sums
g = 1 + 2
h = 3 + 4
setcall(g, h)

# call 9: var and sum
i = 1
j = 2
setcall(i + 1, j + 2)

# call 10: var sum all
k = 1
l = 2
setcall(k + l, l + k)

